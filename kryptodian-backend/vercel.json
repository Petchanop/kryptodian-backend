{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "src/(.*)",
      "dest": "/api",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH",
        "OPTIONS",
        "HEAD",
        "CONNECT",
        "TRACE"
      ]
    },
    {
      "src": "src/auth/(.*)",
      "dest": "/api/auth/register",
      "methods": [
        "POST"
      ]
    }
  ]
}