{
  "version": 2,
  "build":   {
    "src": "src/vercel-func.js",
    "use": "@vercel/node"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/vercel-func.ts",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH",
        "OPTIONS",
        "HEAD",
        "CONNECT",
        "TRACE"
      ]
    }
  ]
}