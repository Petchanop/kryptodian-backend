{
  "version": 2,
  "builds": [
    {
      "src": "src/(.*)",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/(.*)",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH",
        "OPTIONS",
        "HEAD",
        "CONNECT",
        "TRACE"
      ]
    }
  ]
}